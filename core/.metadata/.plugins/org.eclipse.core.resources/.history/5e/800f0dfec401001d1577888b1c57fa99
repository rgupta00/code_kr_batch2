package com.a.io_examples.ex_overriding_ctr.doubt2;

class A {
	public static void foo() {
		System.out.println("foo of class A");
	}
}

class B extends A {
	public static void foo() {
		System.out.println("foo of class B");
	}
}

public class DemoDoubs {

	public static void main(String[] args) {

	}

}
